package com.serefacet.multithreading;

public class TimeBomb extends Thread {
	
	String[] timeStr = {"Zero","One","Two","Three","Four","Five","Six","Seven","Eight","Nine"};
	@Override
	public void run() {
		for (int i = 9; i >= 0; i--) {

			try {
				System.out.println(timeStr[i]);
				Thread.sleep(1000);
			} catch (InterruptedException ie) {
				ie.printStackTrace();
			}
		}

	}
}